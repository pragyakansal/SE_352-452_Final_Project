<+><!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <title>Manage Your Properties</title>\n    <!-- The below line loads in styling elements from the CSS file -->\n    <!-- The @{} syntax is used to resolve the URL of the CSS file -->\n    <!-- Comment it out to see the content without styling -->\n    <link th:href=\"@{/css/styles.css}\" rel=\"stylesheet\"/>\n</head>\n<body>\n<table>\n    <thead>\n        <tr>\n            <th>Number</th>\n            <th>Title</th>\n            <th>Price</th>\n            <th>Location</th>\n            <th>Size</th>\n            <th>Images</th>\n            <!--th>Favorites</th-->\n            <th>Action</th>\n        </tr>\n    </thead>\n\n    <tbody>\n        <!-- The below line iterates over the properties list -->\n        <!-- The below is similar to \"for each 'property' in the 'properties' -->\n        <tr th:each=\"property, propStat : ${properties}\">\n            <td th:text=\"${propStat.count}\"></td> <!-- Good for counting, starts at 1 -->\n            <td th:text=\"${property.title}\">Property Title</td>\n            <td th:text=\"${'$' + #numbers.formatDecimal(property.price, 1, 'COMMA', 2, 'POINT')}\">$0.00</td>\n            <td th:text=\"${property.location}\">Location</td>\n            <td th:text=\"${property.size + ' sq ft'}\">0 sq ft</td>\n            <td>\n                <!-- Display the first image if available -->\n                <img th:if=\"${!property.images.empty}\"\n                     th:src=\"@{/images/{folder}/{file}(folder=${property.title},file=${property.images[0].imageFileName})}\"\n                     alt=\"Property Image\" width=\"100\">\n                <!-- If no images, show a placeholder -->\n                <span th:unless=\"${!property.images.empty}\">No Image</span>\n            <td th:text=\"${property.description}\">Short description...</td>\n            <td>\n                <!-- Link to edit the property -->\n                <a th:href=\"@{/agent/manage-listings/edit/{id}(id=${property.id})}\" class=\"button-link\">Edit</a>\n                <!-- Link to delete the property -->\n                <a th:href=\"@{/agent/manage-listings/delete/{id}(id=${property.id})}\" class=\"button-link red\">Delete</a>\n            </td>\n        </tr>\n    </tbody>\n    </thead>\n</table>\n\n</body>\n</html>
